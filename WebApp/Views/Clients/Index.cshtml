@{
    ViewData["title"] = "Cliente";
}

<section class="content">

    <!-- Default box -->
    <div class="card">
        <div class="card-body p-0">
            <table class="table table-striped projects">
                <thead>
                    <tr>
                        <th style="width: 1%">
                            #
                        </th>
                        <th style="width: 20%">
                            Nome
                        </th>
                        <th style="width: 30%">
                            Endereço
                        </th>
                        <th style="width: 20%">
                            Cnpj
                        </th>
                        <th style="width: 10%">
                        </th>
                    </tr>
                </thead>

                <tbody>
                    <tr>
                        <td>
                            #
                        </td>
                        <td>
                            <a>
                                Nome Fantasia
                            </a>
                            <br />
                            <small>
                                Razão Social
                            </small>
                        </td>
                        <td>
                            <ul class="list-unstyled">
                                <li class="list-inline-item">
                                    Logradouro
                                </li>
                                <li class="list-inline-item">
                                    nº
                                </li>
                                <li class="list-inline-item">
                                    Bairro
                                </li>
                                <li>
                                    Complemento
                                </li>
                                <li class="list-inline-item">
                                    Cidade
                                </li>
                                <li class="list-inline-item">
                                    CEP
                                </li>
                            </ul>
                        </td>
                        <td>
                            <p>CNPJ</p>
                        </td>

                        <td class="project-actions text-right">

                            <a class="btn btn-info btn-sm" href="#">
                                <i class="fas fa-pencil-alt">
                                </i>
                                Editar
                            </a>
                            <a class="btn btn-danger btn-sm" href="#">
                                <i class="fas fa-trash">
                                </i>
                                Apagar
                            </a>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
        <!-- /.card-body -->
    </div>
    <!-- /.card -->

</section>
